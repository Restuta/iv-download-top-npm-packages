{
  "name": "npm-most-dependent-upon-downloader",
  "version": "0.0.1",
  "description": "Downloads top N most dependent upon NPM packages from https://www.npmjs.com/browse/depended",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_PATH=./lib node index.js",
    "dev": "cross-env NODE_PATH=./lib nodemon index.js --ext js --ignore data/",
    "eslint": "eslint lib/ --ext .js || true",
    "eslint:fix": "eslint lib/ --ext .js --fix || true",
    "reinstall": "rimraf node_modules && npm i",
    "test": "node test.js"
  },
  "author": "Anton Vynogradenko <restuta8@gmail.com> (http://restuta.me)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.23.1",
    "babel-preset-env": "^1.1.8",
    "babel-preset-stage-2": "^6.3.13",
    "cross-env": "^3.1.3",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "get-folder-size": "~1.0.0",
    "nodemon": "^1.10.2",
    "rimraf": "^2.5.2",
    "run-series": "^1.1.4",
    "tape": "~4.6.0"
  },
  "dependencies": {
    "cheerio": "^0.22.0",
    "little-scraper": "^1.0.0",
    "lodash": "^4.17.4"
  }
}
